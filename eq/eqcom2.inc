C     $Id$
C
      PARAMETER (NSGM=64,NTGM=64,NUGM=128)
C
      PARAMETER (MLM=NSGM*NTGM,MWM=4*NTGM-1)
      PARAMETER (NSGMP=NSGM+1,NTGMP=NTGM+1)
      PARAMETER (NSGPM=NSGM+2,NTGPM=NTGM+2)
      PARAMETER (NXM=2*NSGM)
      PARAMETER (NT=5)
C
      COMMON /EQMSH1/ DSG,DTG
      COMMON /EQMSH2/ SIGM(NSGM),THGM(NTGM),RHOM(NTGM)
      COMMON /EQMSH3/ RMG(NSGM,NTGMP),RGM(NSGMP,NTGM)
      COMMON /EQMSH4/ SIGG(NSGMP),THGG(NTGMP),RHOG(NTGMP)
      COMMON /EQMSH5/ RMM(NTGM,NSGM),ZMM(NTGM,NSGM)
C
      COMMON /EQBND1/ Q(MWM,MLM)
C
      COMMON /EQCEF1/ AA(NSGMP,NTGM),AB(NSGMP,NTGM)
      COMMON /EQCEF2/ AC(NSGM,NTGMP),AD(NSGM,NTGMP)
C
      COMMON /EQPSI1/ PSI(NTGM,NSGM),PSIO(NTGM,NSGM,NT)
      COMMON /EQPSI2/ DELPSI(NTGM,NSGM)
C
      COMMON /EQRHS1/ HJT(NTGM,NSGM),PP(NTGM,NSGM),TT(NTGM,NSGM)
      COMMON /EQRHS2/ HJP1(NTGM,NSGM),HJP2(NTGM,NSGM)
      COMMON /EQRHS3/ HJT1(NTGM,NSGM),HJT2(NTGM,NSGM)
      COMMON /EQRHS4/ RHO(NTGM,NSGM)
      COMMON /EQRHS5/ TJ,RRC,RIPX
C
      COMMON /EQSPL1/ SIGMX(NSGPM),THGMX(NTGPM)
      COMMON /EQSPL2/ PSIST(NTGPM,NSGPM)
      COMMON /EQSPL3/ UPSIST(4,4,NTGPM,NSGPM)
      COMMON /EQSPL4/ HJTST(NTGPM,NSGPM)
      COMMON /EQSPL5/ UHJTST(4,4,NTGPM,NSGPM)
C
      COMMON /EQZBR1/ ZBRF
C
      COMMON /EQPRF1/ XAX(NXM),RPSI(NXM),RPP(NXM),RTT(NXM)
      COMMON /EQPRF2/ RHJP(NXM),RHJT(NXM)
      COMMON /EQPRF3/ RPSINT(NXM,NT)
C
      COMMON /EQNUG1/ RUGQ(NUGM),URGUQ(4,NUGM)
      COMMON /EQNUG2/ RUGJ(NUGM),URGUJ(4,NUGM)
      COMMON /EQNUG3/ RUG1(NUGM),URUG1(4,NUGM)
      COMMON /EQNUG4/ RUG2(NUGM),URUG2(4,NUGM)
      COMMON /EQNUG5/ RUG3(NUGM),URUG3(4,NUGM)
      COMMON /EQNUG6/ RUGB(NUGM),URUGB(4,NUGM)
